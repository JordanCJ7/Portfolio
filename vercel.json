{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "GOOGLE_API_KEY": "@google_api_key",
    "GOOGLE_GENAI_API_KEY": "@google_api_key",
    "GEMINI_RPM_LIMIT": "5",
    "GEMINI_RPD_LIMIT": "20"
  }
}
